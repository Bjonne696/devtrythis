modules = ["nodejs-20", "web"]
run = "npm run dev"

[nix]
channel = "stable-25_05"

[deployment]
run = ["npm", "run", "preview"]
build = ["npm", "run", "build"]
deploymentTarget = "autoscale"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 34039
externalPort = 3001

[[ports]]
localPort = 42791
externalPort = 3000

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Utviklingserver"
author = 45928993
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Server"

[[workflows.workflow]]
name = "Preview Build"
author = 45928993
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run preview"

[[workflows.workflow]]
name = "Server"
author = "agent"

[[workflows.workflow.tasks]]
